{% extends 'base.html.twig' %}

{% block title %}Listado de Fiscalizadores{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    
    
    <table class="table table-sm">
  <thead>
    <tr>
      <th scope="col">CUIT</th>
      <th scope="col">DNI</th>
      <th scope="col">Nombre</th>
      <th scope="col">Matr√≠cula</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    {% for user in datos %}
    <tr>
      <th scope="row">{{ user.cuit }}</th>
      <td>{{ user.dni }}</td>
      <td>{{ user.nombre }}</td>
      <td>{{ user.matricula }}</td>
      <td><a href="{{ path('imprime_show', {'id': user.id}) }}">Imprime</a><td>
      <td><a href="{{ path('descarga_img', {'id': user.id}) }}">Descarga</a><td>
     
    </tr>
    {% endfor %}
  </tbody>
</table>
</div>
{% endblock %}
